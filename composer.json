{
    "name": "tomatom/depoto-php-client",
    "type": "library",
    "description": "Depoto API client",
    "keywords": ["depoto", "wms", "fullfilment", "api"],
    "homepage": "https://depoto.cz/",
    "license": "MIT",
    "autoload": {
        "psr-4": {"Depoto\\": "src/"},
        "psr-0": {"Depoto\\": "src/"},
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "himik/simple-graphql-query-builder": "^0.2.4",
        "psr/log": ">=1.1",
        "psr/log-implementation": "*",
        "psr/http-client": "^1.0",
        "psr/http-client-implementation": "*",
        "psr/http-factory": "^1.0",
        "psr/http-factory-implementation": "*",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "*",
        "psr/simple-cache": "^1.0",
        "psr/simple-cache-implementation": "*"
    },
    "require-dev": {
        "monolog/monolog": "^2.3",
        "nyholm/psr7": "^1.4",
        "symfony/http-client": "^5.3",
        "symfony/cache": "^5.3",
        "phpunit/phpunit": "^9"
    }
}
